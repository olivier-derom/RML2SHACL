@prefix ex: <http://www.example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:dc_creator a sh:PropertyShape ;
    sh:path <http://purl.org/dc/elements/1.1/creator> .

ex:dct_license a sh:PropertyShape ;
    sh:path <http://purl.org/dc/terms/license> .

ex:dct_title a sh:PropertyShape ;
    sh:path <http://purl.org/dc/terms/title> .

ex:geo_Feature a sh:NodeShape ;
    sh:description "This class represents the top-level feature type. This class is equivalent to GFI_Feature defined in ISO 19156:2011, and it is superclass of all feature types" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:class <http://www.opengis.net/ont/geosparql#Geometry> ;
            sh:path <http://www.opengis.net/ont/geosparql#hasGeometry> ] ;
    sh:targetClass <http://www.opengis.net/ont/geosparql#Feature> .

ex:geo_Geometry a sh:NodeShape ;
    sh:description "The class represents the top-level geometry type. This class is equivalent to the UML class GM_Object defined in ISO 19107, and it is superclass of all geometry types." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <http://www.opengis.net/ont/geosparql#Geometry> .

ex:geo_SpatialObject a sh:NodeShape ;
    sh:description "The class spatial-object represents everything that can have a spatial representation. It is superclass of feature and geometry." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:class <http://www.w3.org/2003/01/geo/wgs84_pos#Point> ;
            sh:path <http://www.w3.org/2003/01/geo/wgs84_pos#location> ],
        [ sh:class <http://www.opengis.net/ont/geosparql#SpatialObject> ;
            sh:path <http://www.opengis.net/ont/geosparql#sfContains> ],
        ex:geo_sfContains ;
    sh:targetClass <http://www.opengis.net/ont/geosparql#SpatialObject> .

ex:geo_hasGeometry a sh:PropertyShape ;
    sh:description "A spatial representation for a given feature." ;
    sh:path <http://www.opengis.net/ont/geosparql#hasGeometry> .

ex:ns2_BidEnergySetpointSignal a sh:NodeShape ;
    sh:description "This is the amount of energy from a resource that was bid into a program" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#BidEnergySetpointSignal> .

ex:ns2_BidEnergySignal a sh:NodeShape ;
    sh:description "Signal that represents the price that was bid by the resource" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#BidEnergySignal> .

ex:ns2_BidLoadSetpointSignal a sh:NodeShape ;
    sh:description "This is the amount of load that was bid by a resource into a program" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#BidLoadSetpointSignal> .

ex:ns2_BidLoadSignal a sh:NodeShape ;
    sh:description "Signal that represents the amount of load that was bid by a resource into a program" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#BidLoadSignal> .

ex:ns2_BidPriceAbsoluteSignal a sh:NodeShape ;
    sh:description "This is the price that was bid by the resource" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#BidPriceAbsoluteSignal> .

ex:ns2_BidPricesignal a sh:NodeShape ;
    sh:description "Signal that represents the price that was bid by the resource" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#BidPricesignal> .

ex:ns2_ChargeStateDeltaSignal a sh:NodeShape ;
    sh:description "This is the delta amount of energy that should be contained in a storage resource from where it currently is." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ChargeStateDeltaSignal> .

ex:ns2_ChargeStateMultiplierSignal a sh:NodeShape ;
    sh:description "This is the percentage of full charge that the storage resource should be at." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ChargeStateMultiplierSignal> .

ex:ns2_ChargeStateSetpointSignal a sh:NodeShape ;
    sh:description "This is used to either charge or discharge a certain amount of energy from a storage resource until its charge state reaches a certain level." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ChargeStateSetpointSignal> .

ex:ns2_ChargeStateSignal a sh:NodeShape ;
    sh:description "Signal used  to dispatch storage resources" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ChargeStateSignal> .

ex:ns2_DataReport a sh:NodeShape ;
    sh:description "These reports are used to report actual data that may be measured or calculated." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:class <https://w3id.org/def/openadr#MetadataReport> ;
            sh:path <https://w3id.org/def/openadr#isDescribedIn> ],
        ex:ns2_isSpecifiedIn,
        ex:ns2_specifiesReport ;
    sh:targetClass <https://w3id.org/def/openadr#DataReport> .

ex:ns2_DemandChargePriceAbsoluteSignal a sh:NodeShape ;
    sh:description "This is the demand charge expressed in absolute terms" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#DemandChargePriceAbsoluteSignal> .

ex:ns2_DemandChargePriceMultiplierSignal a sh:NodeShape ;
    sh:description "This is a multiplier to the existing demand charge" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#DemandChargePriceMultiplierSignal> .

ex:ns2_DemandChargePriceRelativeSignal a sh:NodeShape ;
    sh:description "This is a delta change to the existing demand charge" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#DemandChargePriceRelativeSignal> .

ex:ns2_DemandChargeSignal a sh:NodeShape ;
    sh:description "Signal for the demand charge" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#DemandChargeSignal> .

ex:ns2_ElectricityPriceAbsoluteSignal a sh:NodeShape ;
    sh:description "This is the cost of electricity  expressed in absolute terms" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ElectricityPriceAbsoluteSignal> .

ex:ns2_ElectricityPriceMultiplierSignal a sh:NodeShape ;
    sh:description "This is a multiplier to the existing cost of electricity" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ElectricityPriceMultiplierSignal> .

ex:ns2_ElectricityPriceRelativeSignal a sh:NodeShape ;
    sh:description "This is a delta change to the existing price of electricity" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ElectricityPriceRelativeSignal> .

ex:ns2_ElectricityPriceSignal a sh:NodeShape ;
    sh:description "Signal for the price of electricity" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ElectricityPriceSignal> .

ex:ns2_EnergyPriceAbsolutePriceSignal a sh:NodeShape ;
    sh:description "This is the cost of energy expressed in absolute terms" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#EnergyPriceAbsolutePriceSignal> .

ex:ns2_EnergyPriceMultiplierSignal a sh:NodeShape ;
    sh:description "This is a multiplier to the existing cost of energy" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#EnergyPriceMultiplierSignal> .

ex:ns2_EnergyPriceRelativeSignal a sh:NodeShape ;
    sh:description "This is a delta change to the existing price of energy" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#EnergyPriceRelativeSignal> .

ex:ns2_EnergyPriceSignal a sh:NodeShape ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#EnergyPriceSignal> .

ex:ns2_Event a sh:NodeShape ;
    sh:description "An event is a notification from the utility to demand side resources requesting load shed starting at a specific time, over a specified duration, and may include targeting information designating specific resources that should participate in the event" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#ramp_up> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <http://www.w3.org/2006/time#Duration> ] ],
        [ sh:path <https://w3id.org/def/openadr#eventDescriptor> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#EventDescriptor> ] ],
        [ sh:path <https://w3id.org/def/openadr#expectedResponse> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Opt> ] ],
        [ sh:path <https://w3id.org/def/openadr#includesSignal> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Signal> ] ],
        [ sh:class xsd:string ;
            sh:path <https://w3id.org/def/openadr#hasVersion> ],
        [ sh:path <https://w3id.org/def/openadr#recovery> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <http://www.w3.org/2006/time#Duration> ] ],
        [ sh:path <https://w3id.org/def/openadr#hasCreatedDateTime> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:dateTimeStamp ] ],
        ex:ns2_eventDescriptor,
        ex:ns2_hasAreaTarget,
        ex:ns2_hasEventName,
        ex:ns2_hasEventStatus,
        ex:ns2_hasGroupTarget ;
    sh:targetClass <https://w3id.org/def/openadr#Event> .

ex:ns2_EventDescriptor a sh:NodeShape ;
    sh:description "Part of the OpenADR event object that describes metadata about the event, such as program name and event priority" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property ex:ns2_hasModificationDateTime,
        ex:ns2_hasModificationNumber,
        ex:ns2_hasModificationReason,
        ex:ns2_hasPriority,
        ex:ns2_hasRandomization,
        ex:ns2_isResponseRequired,
        ex:ns2_isTestEvent ;
    sh:targetClass <https://w3id.org/def/openadr#EventDescriptor> .

ex:ns2_EventStatus a sh:NodeShape ;
    sh:description "Status of an OpenADR event" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#EventStatus> .

ex:ns2_GreenButtonReport a sh:NodeShape ;
    sh:description "This is usage history that can be transmitted using the well known GREEN BUTTON standard payloads" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#GreenButtonReport> .

ex:ns2_Group a sh:NodeShape ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#appliesTo> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#ServiceArea> ] ] ;
    sh:targetClass <https://w3id.org/def/openadr#Group> .

ex:ns2_HistoryReport a sh:NodeShape ;
    sh:description "This is a type of data report in which the history of the data point values is logged and can be subsequently requested" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#HistoryReport> .

ex:ns2_HistoryUsageReport a sh:NodeShape ;
    sh:description "These are logs of usage data that are typically logged by VENs and can be queried by the VTN" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#HistoryUsageReport> .

ex:ns2_Item a sh:NodeShape ;
    sh:description "Units for the data in the report" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property ex:ns2_hasItemDescription,
        ex:ns2_hasScaleCode ;
    sh:targetClass <https://w3id.org/def/openadr#Item> .

ex:ns2_LoadControlCapacitySignal a sh:NodeShape ;
    sh:description "This is an instruction for the load controller to operate at a level that is some percentage of its maximum load consumption capacity. This can be mapped to specific load controllers to do things like duty cycling. Note that 1.0 refers to 100% consumption. In the case of simple ON/OFF type devices then 0 = OFF and 1 = ON." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadControlCapacitySignal> .

ex:ns2_LoadControlLevelOffsetSignal a sh:NodeShape ;
    sh:description "Discrete integer levels that are relative to normal operations where 0 is normal operations. There is no requirement to link the setpoints to specific load consumption values, but the intention is that the higher the setpoint the less load is consumed. Note that these are controller set points that can be mapped at the VEN side to something as simple as thermostat temperature set point offsets." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadControlLevelOffsetSignal> .

ex:ns2_LoadControlPercentOffset a sh:NodeShape ;
    sh:description "Percentage change from normal operations. The percentage does not refer to specific load consumptions values, but to load control operation levels. The lower the percentage the less load is consumed." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadControlPercentOffset> .

ex:ns2_LoadControlSetpointSignal a sh:NodeShape ;
    sh:description "Load controller set points. There is no requirement to link the setpoints to specific load consumption values. Note that these are generic controller set points and can be mapped at the VEN side to something as simple as specific thermostat temperature set points." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadControlSetpointSignal> .

ex:ns2_LoadControlSignal a sh:NodeShape ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadControlSignal> .

ex:ns2_LoadControlState a sh:NodeShape ;
    sh:description "The LoadControlState attributes are meant to mirror the LOAD_CONTROL signal attributes meaning that for each LOAD_CONTROL signal type there is a corresponding attribute in the LoadControlState of a report." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property ex:ns2_hasCapacity,
        ex:ns2_hasLevelOffset,
        ex:ns2_hasPercentageOffset,
        ex:ns2_hasSetPoint ;
    sh:targetClass <https://w3id.org/def/openadr#LoadControlState> .

ex:ns2_LoadDispatchDeltaSignal a sh:NodeShape ;
    sh:description "This is used to dispatch loads to some offset from an agreed upon baseline. Note that the baseline may be the current power consumption." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadDispatchDeltaSignal> .

ex:ns2_LoadDispatchLevelSignal a sh:NodeShape ;
    sh:description "This is used to specify the load in terms of discrete levels." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadDispatchLevelSignal> .

ex:ns2_LoadDispatchMultiplier a sh:NodeShape ;
    sh:description "This is used to dispatch loads as some multiple of power against some agreed upon baseline. Note that the baseline may be the current power consumption." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadDispatchMultiplier> .

ex:ns2_LoadDispatchSetpointSignal a sh:NodeShape ;
    sh:description "This is used to dispatch loads to a specific amount" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadDispatchSetpointSignal> .

ex:ns2_LoadDispatchSignal a sh:NodeShape ;
    sh:description "Instructions are used to set the load  values that can be expressed in terms of the desired load" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#LoadDispatchSignal> .

ex:ns2_MetadataReport a sh:NodeShape ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property ex:ns2_reportDescriptor ;
    sh:targetClass <https://w3id.org/def/openadr#MetadataReport> .

ex:ns2_Node a sh:NodeShape ;
    sh:description "Element that interacts with resources in the environment" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#sendsOpt> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Opt> ] ],
        [ sh:path <https://w3id.org/def/openadr#sendsReport> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Report> ] ],
        ex:ns2_hasNodeName,
        ex:ns2_hasProfileName,
        ex:ns2_hasRegistrationNumber,
        ex:ns2_hasTransportName,
        ex:ns2_receives,
        ex:ns2_requires,
        ex:ns2_sendsEvent,
        ex:ns2_sendsOpt,
        ex:ns2_sendsReport ;
    sh:targetClass <https://w3id.org/def/openadr#Node> .

ex:ns2_Opt a sh:NodeShape ;
    sh:description "The expected response from the demand side resource owner upon receipt of an event." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#plans> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Schedule> ] ],
        [ sh:path <https://w3id.org/def/openadr#hasMarketContext> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:anyURI ] ] ;
    sh:targetClass <https://w3id.org/def/openadr#Opt> .

ex:ns2_OptIn a sh:NodeShape ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#OptIn> .

ex:ns2_OptOut a sh:NodeShape ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#OptOut> .

ex:ns2_Report a sh:NodeShape ;
    sh:description "Report sent from a VEN to a VTN to report the energy consumption or the status of Resources connected to the VEN" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#hasCreatedDateTime> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:dateTimeStamp ] ],
        [ sh:path <https://w3id.org/def/openadr#hasReportType> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class rdfs:Literal ] ],
        [ sh:class <http://www.w3.org/2006/time#TemporalEntity> ;
            sh:path <https://w3id.org/def/openadr#hasDuration> ],
        [ sh:path <https://w3id.org/def/openadr#isActiveDuringPeriod> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <http://www.w3.org/2006/time#Interval> ] ],
        [ sh:path <https://w3id.org/def/openadr#hasReportId> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class rdfs:Literal ] ],
        [ sh:path <https://w3id.org/def/openadr#starts> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:dateTimeStamp ] ],
        ex:ns2_hasLoadControlState,
        ex:ns2_hasReportName,
        ex:ns2_hasReportPayload,
        ex:ns2_reportDataSource,
        ex:ns2_reportSubject ;
    sh:targetClass <https://w3id.org/def/openadr#Report> .

ex:ns2_ReportDataDescriptor a sh:NodeShape ;
    sh:description "Part of the OpenADR report object that describes the data included in the descriptor report" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ReportDataDescriptor> .

ex:ns2_ReportDataPayload a sh:NodeShape ;
    sh:description "Part of the OpenADR report object that includes the data sent to a node" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property ex:ns2_includesMeasurement ;
    sh:targetClass <https://w3id.org/def/openadr#ReportDataPayload> .

ex:ns2_ReportDescriptor a sh:NodeShape ;
    sh:description "Part of the OpenADR report object that describes metadata about the report" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:class <https://w3id.org/def/openadr#SamplingRate> ;
            sh:path <https://w3id.org/def/openadr#hasSamplingRate> ],
        [ sh:path <https://w3id.org/def/openadr#involvesSubject> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Group> ] ] ;
    sh:targetClass <https://w3id.org/def/openadr#ReportDescriptor> .

ex:ns2_ReportPayload a sh:NodeShape ;
    sh:description "Payload associated to every report" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#hasDataQuality> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:string ] ],
        [ sh:path <https://w3id.org/def/openadr#hasAccuracy> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:string ] ],
        ex:ns2_isDescribedIn ;
    sh:targetClass <https://w3id.org/def/openadr#ReportPayload> .

ex:ns2_ReportStatusDescriptor a sh:NodeShape ;
    sh:description "Part of the OpenADR report object that describes the status of the descriptor report" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ReportStatusDescriptor> .

ex:ns2_ReportStatusPayload a sh:NodeShape ;
    sh:description "Part of the OpenADR report object that includes the status  of the payload report" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property ex:ns2_isManualOverride,
        ex:ns2_isOnline ;
    sh:targetClass <https://w3id.org/def/openadr#ReportStatusPayload> .

ex:ns2_Resource a sh:NodeShape ;
    sh:description "Entity that is enrolled in the DR Programs and is capable of delivering some sort of change to their load profile in response to receiving a DR signal from a VTN." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#Resource> .

ex:ns2_SamplingRate a sh:NodeShape ;
    sh:description "The average number of samples obtained in one second" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#isOnChange> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class xsd:boolean ] ],
        [ sh:path <https://w3id.org/def/openadr#hasItemBase> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Item> ] ],
        [ sh:class <http://www.w3.org/2006/time#TemporalEntity> ;
            sh:path <https://w3id.org/def/openadr#hasMaxPeriod> ],
        [ sh:class <http://www.w3.org/2006/time#TemporalEntity> ;
            sh:path <https://w3id.org/def/openadr#hasMinPeriod> ],
        ex:ns2_isOnChange ;
    sh:targetClass <https://w3id.org/def/openadr#SamplingRate> .

ex:ns2_Schedule a sh:NodeShape ;
    sh:description "Schedule of a target" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#refersToTarget> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#Group> ] ],
        [ sh:path <https://w3id.org/def/openadr#isUnavailableDuring> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <http://www.w3.org/2006/time#Interval> ] ],
        [ sh:path <https://w3id.org/def/openadr#isAvailableDuring> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <http://www.w3.org/2006/time#Interval> ] ],
        ex:ns2_isScheduleOf ;
    sh:targetClass <https://w3id.org/def/openadr#Schedule> .

ex:ns2_ServiceArea a sh:NodeShape ;
    sh:description "The Service Area expresses locations or geographic regions relevant to price communication" ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:targetClass <https://w3id.org/def/openadr#ServiceArea> .

ex:ns2_Signal a sh:NodeShape ;
    sh:description "The actionable information contained in an event such as electricity pricing or specific levels of load shed requested that typically trigger some preprogrammed load shed behavior by the recipient of the event." ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:property [ sh:path <https://w3id.org/def/openadr#hasSignalType> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#SignalType> ] ],
        [ sh:path <https://w3id.org/def/openadr#isDefinedIn> ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class <https://w3id.org/def/openadr#SignalInterval> ] ],
