        [ rdfs:label "has transport name" ;
            sh:datatype xsd:string ;
            sh:name "has transport name" ;
            sh:nodeKind sh:Literal ;
            sh:path ns:hasTransportName ],
        [ rdfs:label "has profile name" ;
